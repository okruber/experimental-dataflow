### To run in OG-mode:
    python pipeline/main.py \
  --input_path gs://totemic-bucket-raw/important_file_20250220.txt \
  --output_path gs://totemic-bucket-trusted/important_file_20250220.txt \
  --runner DataflowRunner \
  --project olle-kruber-project \
  --region europe-north1 \
  --temp_location gs://totemic-dataflow-temp

### Prep for Flex-templates:

gcloud compute networks subnets update default \
	--region=europe-north1 \
	--enable-private-ip-google-access
##

gcloud artifacts repositories create solita-dataflow-image \
    --repository-format=docker \
    --location=europe-north1 \
    --async

